/* pages/home/home.wxss */

.home-top-box {
    height: 200rpx;
    background-color: #4dcbca;
    color:rgba(255,255,255,1);

}

.h-t-b-inner {
    height: 112rpx;
    padding-top: 44rpx;
}

.h-t-b-inner>view{
    float: left;
    font-weight:400;
    font-size: 24rpx;
}

.head-img {
    width: 112rpx;
    height: 112rpx;
    background-color: aliceblue;
    margin:0 24rpx 0 50rpx;
    border-radius: 50%;
}

.h-i-img{
    width: 100%;
    height: 100%;
    border-radius: 50%;
    overflow: hidden;
}

.head-greet {
    width: 50%;
    height: 77rpx;
    margin: 17rpx 0 18rpx;
}



.h-g-title{
    font-size: 32rpx;
    display: block;
}

.h-g-title.small{
    font-size: 28rpx;
}

.logout-btn{
    width: 120rpx;
    height: 50rpx;
    border: 1px solid;
    margin: 46rpx 31rpx 16rpx 0;
    float: right !important;
    line-height: 50rpx;
    text-align: center;
    font-size: 24rpx;
    border-radius: 15rpx;
}

.entrance-check{
    display: flex;
    flex-direction: row;
    flex-wrap:wrap;
    justify-content: space-around;
    margin-bottom: 100rpx;
}


.e-check-item{
    display: inline-block;
    border: 1px solid #4dcbca;
    border-radius: 33rpx;
    box-shadow: 0rpx 0rpx 15rpx  #4dcbca;
    width: 316rpx;
    height: 322rpx;
    margin-top: 37rpx;
    overflow: hidden;
}

.e-check-item.e-check-disable{
    border-color: #959594;
    box-shadow: 0rpx 0rpx 15rpx  #959594;
    color: #959594;
}

.e-c-i-label{
    height: 95rpx;
    line-height: 95rpx;
    padding-left: 29rpx;
    font-size:40rpx;
    color: rgba(51, 51, 51, 1);
    font-weight:400;

}

.e-c-i-svg{
    width: 240rpx;
    height: 240rpx;
    float: right;
}

.e-c-i-svg-borrow{
    background: #fff url(data:image/svg+xml;base64,) no-repeat;

}

.e-c-i-svg-return{
    background: #fff url(data:image/svg+xml;base64,) no-repeat;
}

.e-c-i-svg-manage{
    background: #fff url(data:image/svg+xml;base64,) no-repeat;
}

.e-c-i-svg-search{
    background: #fff url(data:image/svg+xml;base64,) no-repeat;
}


.e-c-i-load{
    text-align: center;
    height: 322rpx;
    line-height: 322rpx;
    font-size:40rpx;
    font-weight:400;
}















